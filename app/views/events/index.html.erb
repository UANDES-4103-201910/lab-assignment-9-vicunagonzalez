<h1>Events</h1>
<table>
  <thead>
  <tr>
    <th colspan="4"></th>
  </tr>
  </thead>

  <tbody>
  <% @events.each do |event| %>
    <% place = Place.find_by(id: event.place) %>
    <tr>
      <td><%= event.name %></td>
      <td><%= event.description %></td>
      <td><%= event.start_date %></td>
      <td><%= event.place.name %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', events_new_path %>
